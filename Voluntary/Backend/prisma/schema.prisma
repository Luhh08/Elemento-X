// Aqui iremos controlar todas as configurações do banco de dados
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
// Aqui é uma tabela do banco de dados, chamada de Model
model Usuario {

  id      String @id @default(auto()) @map("_id") @db.ObjectId
  nome    String
  usuario String
  email   String @unique
  cpf     String @unique
  senha   String
  
}