// Aqui iremos controlar todas as configurações do banco de dados
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("Voluntary/Backend/prisma\bancodedados.db")
}

model Voluntario {

  id    String @id @default(uuid()) @map("_id") @db.ObjectId 
  nome String
  usuario String
  cpf   Int @unique 
  email String @unique 
  senha String 

}